    "eslint": "7.32",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.1.1",
    "mocha": "^9.1.3",
